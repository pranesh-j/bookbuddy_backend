{
    "builds": [
        {
            "src": "booksbuddy_backend/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "booksbuddy_backend/wsgi.py"
        },
        {
            "src": "/(.*)",
            "dest": "booksbuddy_backend/wsgi.py"
        }
    ]
}